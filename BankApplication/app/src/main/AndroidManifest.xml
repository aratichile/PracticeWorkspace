<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chilaa_cont.bankapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.BankingLinkActivity"
            android:label="@string/title_activity_banking_link"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".activity.AddBankAccountActivity"
            android:label="@string/title_activity_add_bank_account"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".activity.BankAccountListActivity"
            android:label="@string/title_activity_bank_account_list"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".activity.BankingDetailActivity"
            android:label="@string/title_activity_banking_detail"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".activity.BankingIdeasActivity"
            android:label="@string/title_activity_banking_ideas"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".activity.BankAccountHolderDetailActivity"
            android:label="@string/title_activity_bank_account_holder_detail"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>